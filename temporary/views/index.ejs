<%- include('partials/header.ejs') %>
 
<main>
    <ul>
        <% for (const pokemon of pokedexJson) {%>
        <li>
            <a href="/detail/<%= pokemon.numero %>">
                <img src="/img/<%= pokemon.numero %>.png" alt="<%= pokemon.nom %>">
                <h2>#<%= pokemon.numero %> <%= pokemon.nom %></h2>
            </a>
        </li>
        <% } %>
    </ul>
</main>

</body>
</html>